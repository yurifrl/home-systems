$TTL 60 ; 1 minute
@       IN      SOA     ns.syscd.dev. root.syscd.dev. (
                        16         ; serial
                        60         ; refresh (1 minute)
                        60         ; retry (1 minute)
                        60         ; expire (1 minute)
                        60         ; minimum (1 minute)
                        )
@       IN      NS      ns.syscd.dev.
ns      IN      A       127.0.0.1
